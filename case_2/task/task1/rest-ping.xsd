<schema elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://lanit.ru/fpsid/rest-ping-1.0" xmlns:tns="http://lanit.ru/fpsid/rest-ping-1.0">
	<element name="ping">
		<complexType>
			<attribute name="value" type="long" use="required">
				<annotation>
					<documentation>В этом аттрибуте передается целочисленное значение,
						которое должно быть увеличено сервером</documentation>
				</annotation>
			</attribute>
		</complexType>
	</element>
</schema>